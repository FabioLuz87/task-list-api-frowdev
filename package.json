{
  "name": "task-list-api-frowdev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "build": "npx tsc",
    "start": "node ./dist/main/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:up": "typeorm-ts-node-commonjs migration:run -d ./src/main/database/database-connection.ts",
    "typeorm:down": "typeorm-ts-node-commonjs migration:revert -d ./src/main/database/database-connection.ts",
    "typeorm:migration:create": "typeorm-ts-node-commonjs migration:create  ./src/main/database/migrations/CreateTableTasks",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.2.4",
    "ioredis-mock": "^8.2.2",
    "jest": "^29.3.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.3"
  }
}
